{
  "name": "media-server-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn run serv-dev\" \"yarn run vite-dev\" ",
    "vite-dev": "vite",
    "vite-local": "set IS_DEV=true&&vite",
    "build-win": "vite build&&move.bat",
    "build-linux": "vite build&&./move.sh",
    "server": "node server/app",
    "serv-dev": "nodemon server/app",
    "updateDb": "set CDB=mediaserverdb&&node db.js updateDb",
    "updateDb-linux": "export CDB=mediaserverdb&&node db.js updateDb",
    "updateAd_Db": "set CDB=mediaserver_adultdb&&node db.js updateDb",
    "saveDb": "node db.js saveDb",
    "testDb": "node db.js test"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.2",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "concurrently": "^7.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "drivelist": "^11.0.0",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "fs-extra": "^10.1.0",
    "mariadb": "^3.0.1",
    "nanoid": "3.3.4",
    "node-stream-zip": "^1.15.0",
    "nodemon": "^2.0.20",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "sequelize": "^6.23.2",
    "sharp": "^0.31.0",
    "socket.io": "^4.5.2",
    "sqlite3": "^5.1.1",
    "svelte": "^3.49.0",
    "vite": "^3.1.0",
    "win-explorer": "file:modules/win-explorer"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "socket.io-client": "^4.5.2",
    "svelte-routing": "file:modules/svelte-routing"
  }
}